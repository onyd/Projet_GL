class A {
    int x = 1, y = 2;

    int getX() {
        return x;
    }

    int getY() {
        return y;
    }
}

class B extends A {
    int x = 3;

    int getX() {
        return x;
    }
}

{
    A a = new A();
    B b = new B();

    println(a.getX());
    println(a.getY());
    println(b.getX());
    println(b.getY());
}